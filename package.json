{
  "name": "@thibaudcolas/eslint-plugin-cookbook",
  "version": "2.2.0",
  "description": "Shareable ESLint config, based on airbnb/javascript",
  "license": "MIT",
  "author": "Thibaud Colas",
  "main": "./src/config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thibaudcolas/eslint-plugin-cookbook"
  },
  "bugs": {
    "url": "https://github.com/thibaudcolas/eslint-plugin-cookbook/issues"
  },
  "homepage": "https://github.com/thibaudcolas/eslint-plugin-cookbook",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "src/"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "peerDependencies": {
    "eslint": "^5.6.0"
  },
  "scripts": {
    "linter:js": "eslint",
    "formatter:js": "prettier --write",
    "lint": "npm run linter:js -s -- lib",
    "format": "npm run formatter:js -s -- lib/**/*.js",
    "build": "./lib/buildConfigs.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "npm run build -s && npm run lint -s && npm run test:coverage -s",
    "report:coverage": "open coverage/lcov-report/index.html"
  }
}
