{
  "name": "@thibaudcolas/eslint-plugin-cookbook",
  "version": "3.0.0",
  "description": "Shareable ESLint config, based on airbnb/javascript, bundled as a plugin",
  "license": "MIT",
  "author": "Thibaud Colas",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thibaudcolas/eslint-plugin-cookbook.git"
  },
  "bugs": {
    "url": "https://github.com/thibaudcolas/eslint-plugin-cookbook/issues"
  },
  "homepage": "https://github.com/thibaudcolas/eslint-plugin-cookbook",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "src/"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/exec": "^3.1.3",
    "@semantic-release/git": "^7.0.4",
    "danger": "^4.0.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "semantic-release": "^15.9.15"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "peerDependencies": {
    "eslint": "^5.6.0"
  },
  "scripts": {
    "lint": "eslint . && prettier --list-different '**/*.{js,md,css,scss,yaml,yml,json}'",
    "format": "prettier --write '**/*.{js,md,css,scss,yaml,yml,json}'",
    "build": "./src/buildConfigs.js",
    "build:docs": "node src/documentation.js",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "npm run build && npm run lint && npm run test:coverage",
    "report:coverage": "open coverage/lcov-report/index.html",
    "danger": "danger ci --verbose",
    "semantic-release": "semantic-release"
  }
}
